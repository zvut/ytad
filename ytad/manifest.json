{
  "manifest_version": 2,
  "name": "yt1",
  "description": "yt1",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "run_at": "document_idle",
      "matches": [
        "<all_urls>"
      ]
